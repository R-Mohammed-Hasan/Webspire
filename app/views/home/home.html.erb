    <!-- carousel to view post -->

    <div class="postsContainer">
        <!-- post.each |page|  and then render the single page html -->
        <div class="post-container">
            <div class="post-info">
                <div class="user-profile">
                    <a href="user/:id">
                        <img src="<%= asset_path "user-default-icon.png" %>" width="100%" alt="">
                    </a>
                </div>
                <div class="username">
                    <a href="user/:id">
                            The DON of ...
                        </a>
                </div>
                <div class="more-option" onclick="showMoreOption()">
                    <img src="<%= asset_path "more-icon.png" %>" width="100%" alt="">
                </div>
            </div>

            <div class="post-content">
                <div>
                    <img src="data:image/jpeg;base64,"
                        alt="">
                </div>
            </div>

            <div class="post-credits">
                <div class="credits">
                    <div class="likes-container">
                        <img class="like" title="Like this post" src="<%= asset_path "like-icon.png" %>" alt="">
                        <p class="likes-count">2378</p>
                    </div>
                    <img class="comment" title="Comments" src="<%= asset_path "comment-icon.png" %>" alt="">
                    <img class="bookmark" title="Bookmark this" src="<%= asset_path "bookmark-icon.png" %>" alt="">
                </div>

                <div class="post-description-container">
                    <p class="post-description">
                        <strong> Description :</strong> This is a very good info about css overflow Lorem ipsum dolor sit, amet consectetur adipisicing elit. Ad cupiditate modi amet officia earum magni nihil necessitatibus, facere aliquam assumenda
